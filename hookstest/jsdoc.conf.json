{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "./node_modules/better-docs/component"],
    "templates": {
      "better-docs": {
        "name": "React Dynamic Json Table"
      }
    },
    "opts": {
        "encoding": "utf8",
      "destination": "docs",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/better-docs",
      "readme": "README.md"
    }
  }